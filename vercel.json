{
    "version": 2,
    "builds": [
      {
        "src": "public",
        "use": "@vercel/static-build"
      },
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "public/index.html"
      }
    ]
  }
  